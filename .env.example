# Backend (Express)
PORT=5000
NODE_ENV=development
DATABASE_URL=postgresql://USER:PASSWORD@localhost:5432/frethan_db?schema=public

# Auth (JWT)
JWT_SECRET=
JWT_EXPIRES_IN=7d

# Frontend redirect target after successful OAuth
# This must match your frontend route (see frontend/src/pages/OAuthHandler.tsx)
FRONTEND_OAUTH_REDIRECT=http://localhost:5173/oauth/callback

# OAuth (placeholders for W4)
# Google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:5000/api/auth/google/callback
GOOGLE_SCOPES=openid,email,profile

# WeChat (demo)
WECHAT_APP_ID=
WECHAT_APP_SECRET=
WECHAT_REDIRECT_URI=http://localhost:5000/api/auth/wechat/callback
WECHAT_SCOPES=snsapi_login

# Frontend (Vite) lives in frontend/.env.example
